{
  "name": "mangosteen",
  "version": "0.0.1",
  "description": "alqmc 模板生成工具",
  "scripts": {
    "build": "gulp --require sucrase/register/ts --gulpfile build/gulpfile.ts",
    "clear": "rimraf dist",
    "link": "cd dist && npm link",
    "push": "git push gitee master && git push github master",
    "update:version": "sucrase-node build/utils/version.ts",
    "log": "conventional-changelog -p custom-config  -i CHANGELOG.md -s -n ./changelog-option.js",
    "prepare": "husky install"
  },
  "author": "alqmc",
  "license": "MIT",
  "devDependencies": {
    "@alqmc/build-ts": "^0.0.8",
    "@alqmc/build-utils": "^0.0.3",
    "@alqmc/eslint-config": "^0.0.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/eslint": "^8.4.3",
    "@types/gulp": "^4.0.9",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.21",
    "@types/prompts": "^2.0.14",
    "commitizen": "^4.1.2",
    "compare-func": "^2.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "sucrase": "^3.20.3",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "kolorist": "^1.5.1",
    "minimist": "^1.2.6",
    "prompts": "^2.4.2"
  }
}